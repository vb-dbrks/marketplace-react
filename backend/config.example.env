# Example environment variables for Data Marketplace
# Copy this file to .env and update with your actual values

# Lakebase Database Configuration
# Get these from your Databricks workspace -> Apps -> Your App -> Database resource
PGHOST=your-lakebase-host.database.azuredatabricks.net
PGUSER=your-email@databricks.com
PGDATABASE=databricks_postgres
PGPORT=5432
PGSSLMODE=require
PGPASSWORD=your-databricks-token-here

# Application Configuration
DATABRICKS_APP_PORT=8000
ENVIRONMENT=development
